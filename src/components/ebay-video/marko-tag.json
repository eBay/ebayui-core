{
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attributes": "expression",
  "@cdn-url": "string",
  "@play-view": {
    "enum": ["inline", "fullscreen"]
  },
  "@cdn-version": "string",
  "@a11y-play-text": "string",
  "@a11y-load-text": "string",
  "@report-text": "string",
  "@error-text": "string",
  "@volume": "number",
  "@volume-slider": "boolean",
  "@spinner-timeout": "number",
  "@muted": "boolean",
  "@sources <source>[]": {
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@src": "string",
    "@type": "string"
  },
  "@tracks <track>[]": {
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@src": "string",
    "@type": "string"
  }
}
