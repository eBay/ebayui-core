<!-- patch render function to get access to page template -->
<script marko-init>
var templateProto = Object.getPrototypeOf(module.exports);
var _render = templateProto.render;
templateProto.render = function (data, out) {
    var globals = out.global || data.$global || (data.$global = {});
    globals.pageTemplate = globals.pageTemplate || this;
    return _render.apply(this, arguments);
}
</script>

<svg hidden if(data.hasIcons)>
    <include-html("./svg/arrow-left.html") if(data.icons['arrow-left'])/>
    <include-html("./svg/arrow-right.html") if(data.icons['arrow-right'])/>
    <include-html("./svg/arrow-move.html") if(data.icons['arrow-move'])/>
    <include-html("./svg/arrow-right-bold.html") if(data.icons['arrow-right-bold'])/>
</svg>
