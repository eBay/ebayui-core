{
  "template": "./index.marko",
  "transformer": "./transformer.js",
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attributes": "expression",
  "@type": {
    "enum": ["inline", "background"]
  },
  "@name": "string",
  "@a11y-text": "string",
  "@no-skin-classes": "boolean",
  "@_themes": "expression",
  "@role": "never",
  "@aria-labelledby": "never"
}
