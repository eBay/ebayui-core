<if(data.type === "background")>
    <span w-bind w-body ${data.htmlAttributes} class=data.classes/>
</if>
<else-if(data.type === "inline")>
    <svg w-bind ${data.htmlAttributes} aria-hidden="true" class=data.classes focusable="false">
        <use xlink:href="#icon-${data.name}"/>
    </svg>
</else-if>
