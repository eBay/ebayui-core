{
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attributes": "expression",
  "@selected-index": "number",
  "@tab-aria-current": {
    "enum": ["page", true]
  },
  "@tabs <tab>[]": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression",
    "@href": "#html-href"
  }
}
