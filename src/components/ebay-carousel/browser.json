{
    "dependencies": [
        {
            "if-not-flag": "ebayui-no-skin",
            "dependencies": [
                "@ebay/skin/utility",
                "@ebay/skin/less",
                "@ebay/skin/icon"
            ]
        },
        "require-run: nodelist-foreach-polyfill",
        "require: ./index.js",
        "./style.less",
        {
            "if-flag": "touch",
            "dependencies": [
                "./style-touch.less"
            ]
        },
        {
            "if": "flags.contains('no-touch') || !flags.contains('touch')",
            "dependencies": [
                "./style-no-touch.less"
            ]
        }
    ]
}
