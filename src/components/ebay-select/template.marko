<span class=data.class style=data.style w-bind>
    <select
        disabled=data.disabled
        w-onchange="optionChanged"
        ${data.htmlAttributes}>
        <option
            for(option in data.options)
            value=option.value
            selected=option.selected
            class=option.class
            style=option.style
            ${option.htmlAttributes}>
            ${option.label}
        </option>
    </select>
    <span class="select__icon"/>
</span>
