<span
    class=data.class
    ${data.htmlAttributes}
    w-bind>
    <select
        name=data.name>
        <option
            for(option in data.options)
            value=option.value
            selected=option.selected
            class=option.class
            ${option.htmlAttributes}>
            ${option.label}
        </option>
    </select>
</span>
