{
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attributes": "expression",
  "@layout": { "enum": ["row", "column"] },
  "@open": "boolean",
  "@action <action>": {
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression",
    "@action-text": "string",
    "@action-access-key": "string"
  }
}
