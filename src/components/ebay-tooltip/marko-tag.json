{
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attributes": "expression",
  "@pointer": {
    "enum": [
      "top-left",
      "top",
      "top-right",
      "right",
      "right-bottom",
      "right-top",
      "bottom-left",
      "bottom-right",
      "bottom",
      "left",
      "left-bottom",
      "left-top"
    ]
  },
  "@style-top": "string",
  "@style-left": "string",
  "@style-right": "string",
  "@style-bottom": "string",
  "@no-hover": "boolean",
  "@heading <heading>": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression"
  },
  "@host <host>": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression"
  },
  "@content <content>": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression"
  }
}
