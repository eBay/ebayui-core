{
    "attribute-groups": ["html-attributes"],
    "@*": {
        "targetProperty": null,
        "type": "expression"
    },
    "@html-attributes": "expression",
    "@size": {
        "enum": ["regular", "large"]
    },
    "@as": "string",
    "@auto-collapse": "boolean",
    "@a11y-role-description": "string",
    "@items <item>[]": {
        "attribute-groups": ["html-attributes"],
