{
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attrbibutes": "expression",
  "@variant": {
    "enum": ["form"]
  },
  "@text": "string",
  "@footer-text": "string",
  "@disabled": "boolean",
  "@form-name": "string",
  "@form-action": "string",
  "@form-method": "string",
  "@a11y-text": "string",
  "@a11y-footer-text": "string",
  "@items <item>[]": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression",
    "@checked": "boolean",
    "@value": "string",
    "@role": "never",
    "@aria-checked": "never",
    "@for": "never"
  },
  "@footer <footer>": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression"
  }
}
