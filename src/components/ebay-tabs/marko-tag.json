{
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attributes": "expression",
  "@selected-index": "number",
  "@activation": {
    "enum": ["auto", "manual"]
  },
  "@tabs <tab>[]": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression"
  },
  "@panels <panel>[]": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression",
    "@id": "never",
    "@aria-labelledby": "never",
    "@role": "never",
    "@hidden": "never"
  }
}
