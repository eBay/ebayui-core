{
  "attribute-groups": ["html-attributes"],
  "@*": {
    "targetProperty": null,
    "type": "expression"
  },
  "@html-attributes": "expression",
  "@index": "string",
  "@fake": "boolean",
  "@activation": {
    "enum": ["auto", "manual"]
  },
  "@headings <heading>[]": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression",
    "@href": "#html-href"
  },
  "@panels <panel>[]": {
    "attribute-groups": ["html-attributes"],
    "@*": {
      "targetProperty": null,
      "type": "expression"
    },
    "@html-attributes": "expression",
    "@id": "never",
    "@aria-labelledby": "never",
    "@role": "never",
    "@hidden": "never"
  }
}
