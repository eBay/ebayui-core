<if(!state.dismissed)>
    <ebay-notice-base
        ...input
        role='region'
        prefix-class='section-notice'
        type='section'
        main-root='span'
        a11yRoleDescription=input.a11yRoleDescription || 'Notice'
        class=[input.status === 'education' && 'section-notice--large-icon', input.class]
        on-dismiss('onDismiss')
    />
</if>
