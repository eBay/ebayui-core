{
  "name": "@ebay/ebayui-core",
  "version": "10.0.0-1",
  "description": "Collection of core eBay components; considered to be the building blocks for all composite structures, pages & apps.",
  "scripts": {
    "installMarkoV5": "npm i marko@5 @marko/compiler@5 --force",
    "installMarkoV4": "npm remove @marko/compiler && npm i marko@4 --force",
    "prepublishOnly": "node scripts/prepublish",
    "version": "npm run build:storybook && npm run build:readme && git add -A src docs",
    "postpublish": "node scripts/postpublish",
    "start": "start-storybook -p 6006",
    "start:prod": "NODE_ENV=production npm start",
    "coverage": "nyc marko-test && node scripts/coverage",
    "report": "nyc report --reporter=text-summary",
    "test": "npm run coverage && npm run report",
    "test:server": "marko-test --server",
    "test:browser": "marko-test --browser",
    "test:fps": "karma start",
    "test:browserstack": "env $(cat .env | xargs) npm run tb",
    "clean": "rimraf .coverage .karma .nyc_output demo/static",
    "clean:git": "git clean -xdf",
    "lint": "npm run lint:less && npm run lint:js && npm run lint:whitespace",
    "lint:less": "stylelint '**/**.less'",
    "lint:js": "eslint .",
    "lint:whitespace": "lintspaces *.md src/**/*.md src/**/*.marko 'src/**/!(*.marko).js' src/**/*.less --newline --maxnewlines 1 --trailingspaces --spaces 4",
    "build": "npm run lint && node scripts/check-postpublish && npm test",
    "build:ci": "npm run lint && node scripts/check-postpublish && npm test && npm run build",
    "build:readme": "node scripts/add-ds-version",
    "build:storybook": "DS=6 build-storybook -o ./docs && DS=4 build-storybook -o ./docs/ds4",
    "importSVG": "node --experimental-json-modules --experimental-import-meta-resolve scripts/import-svg",
    "ts": "npm run test:server",
    "tb": "npm run test:browser",
    "tf": "npm run test:fps",
    "tr": "npm run coverage && npm run report && open .coverage/index.html",
    "storybook": "start-storybook -p 6006",
    "generateCDN": "node scripts/generate-cdn.js",
    "prepare": "husky install"
  },
  "browserslist": [
    "extends @ebay/browserslist-config"
  ],
  "files": [
    "dist",
    "!*.marko.js",
    "!**/test*",
    "marko.json",
    "package-lock.json",
    "*.browser.json",
    "marketplace.json"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eBay/ebayui-core"
  },
  "keywords": [
    "marko-components"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.17.12",
    "@babel/runtime-corejs3": "^7.18.3",
    "@commitlint/cli": "^17",
    "@commitlint/config-conventional": "^17",
    "@ebay/browserslist-config": "^2.2.0",
    "@ebay/skin": "~15.0.0-1",
    "@lasso/marko-taglib": "^2.0.1",
    "@marko/compiler": "^5.22.6",
    "@marko/migrate": "^6.0.2",
    "@marko/prettyprint": "^3.0.1",
    "@marko/test": "^9",
    "@marko/testing-library": "^6",
    "@mdx-js/react": "^2.1.3",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/marko": "~7.4.4",
    "@storybook/mdx2-csf": "^0.0.3",
    "@wdio/browserstack-service": "^7.23.0",
    "async": "^3.2.4",
    "babel-loader": "^8.2.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "4.3.6",
    "chai-dom": "^1.10.0",
    "cheerio": "^1.0.0-rc.11",
    "chromedriver": "^104",
    "coveralls": "^3.1.1",
    "css-loader": "^6",
    "dotenv-webpack": "^8",
    "eslint": "^8",
    "eslint-config-ebay": "^1.1.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^4",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-mocha": "^10.0.5",
    "html-minifier": "^4.0.0",
    "husky": "^8.0.1",
    "is-travis": "^2.0.0",
    "lasso": "^3.4.3",
    "lasso-babel-transform": "^3.0.1",
    "lasso-less": "^4.0.1",
    "lasso-marko": "^3.1.1",
    "less": "^4.1.0",
    "less-loader": "^11",
    "lint-staged": "^13.0.2",
    "lintspaces-cli": "^0.7.1",
    "marko": "^5",
    "mocha": "^10",
    "mocha-snap": "^4.3.0",
    "nyc": "^15.1.0",
    "postcss-less": "^6.0.0",
    "pre-push": "^0.1.1",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "shaka-player": "4.2.0",
    "sinon": "^14.0.0",
    "style-loader": "^3",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28",
    "stylelint-prettier": "^2",
    "wdio-chromedriver-service": "^7"
  },
  "peerDependencies": {
    "@ebay/skin": "~15.0.0-1",
    "marko": "^4 || ^5"
  },
  "dependencies": {
    "@marko-tags/subscribe": "^0.4.2",
    "makeup-active-descendant": "0.5.1",
    "makeup-expander": "~0.10.0",
    "makeup-floating-label": "~0.3.1",
    "makeup-focusables": "~0.3.0",
    "makeup-key-emitter": "~0.3.0",
    "makeup-keyboard-trap": "~0.4.1",
    "makeup-prevent-scroll-keys": "~0.2.0",
    "makeup-roving-tabindex": "~0.5.1",
    "makeup-screenreader-trap": "~0.4.1",
    "makeup-typeahead": "^0.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "lint-staged": {
    "*.{js,css,md,less,json}": "prettier --write"
  }
}
